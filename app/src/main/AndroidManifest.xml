<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tema3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tema3">
        <activity android:name=".activities.AuthenticationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/dashboard_activity_name" android:name=".activities.DashboardActivity"/>
        <activity android:label="@string/topics_activity_name" android:name=".activities.TopicsActivity"/>
        <activity android:label="@string/management_activity_name" android:name=".activities.ManagementActivity"/>
    </application>

</manifest>